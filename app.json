{
  "expo": {
    "plugins": [
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#1e1b4b",
            "image": "./assets/adaptive-icon.png",
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#1e1b4b",
            "image": "./assets/adaptive-icon.png",
            "imageWidth": 150
          }
        }
      ]
    ],
    "name": "VocabAI",
    "slug": "vocabai",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enesseval.vocabai",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.enesseval.vocabai"
    },
    "extra": {
      "eas": {
        "projectId": "d279367b-d53c-4af1-becf-07204e350c80"
      }
    }
  }
}